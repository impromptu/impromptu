{
  "name": "impromptu",
  "description": "A sentient prompt.",
  "version": "0.2.0-alpha",
  "homepage": "http://impromptu.sh/",
  "author": "Impromptu Team (http://impromptu.sh/)",
  "contributors": [
    "Daryl Koopersmith (http://darylkoop.com/)",
    "Evan Solomon (http://evansolomon.me/)"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/Impromptu/impromptu.git"
  },
  "bugs": {
    "url": "https://github.com/impromptu/impromptu/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/impromptu/impromptu/blob/master/LICENSE-MIT"
    }
  ],
  "main": "lib/impromptu",
  "engines": {
    "node": ">= 0.8.0"
  },
  "bin": {
    "impromptu": "./bin/impromptu",
    "impromptu-background": "./bin/impromptu-background"
  },
  "preferGlobal": true,
  "scripts": {
    "test": "mocha test/*.coffee --compilers coffee:coffee-script --reporter spec",
    "postinstall": "summon redis --testCommand=redis-server",
    "prepublish": "./scripts/bootstrap"
  },
  "dependencies": {
    "coffee-script": "~1.6.2",
    "redis": "~0.8.2",
    "async": "~0.2.6",
    "underscore": "~1.4.4",
    "summon": "~0.2.0"
  },
  "devDependencies": {
    "grunt-contrib-watch": "~0.3.1",
    "grunt-contrib-coffee": "~0.6.0",
    "grunt": "~0.4.1",
    "mocha": "~1.8.2",
    "should": "~1.2.2"
  },
  "keywords": []
}
